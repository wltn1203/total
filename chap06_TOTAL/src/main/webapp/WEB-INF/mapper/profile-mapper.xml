<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="profile">

	<insert id="addBasic">
		insert into profiler values(profiler_seq.nextval, #{id}, #{url}, sysdate)		
	</insert>
	
	<select id="readAll" resultType="java.util.HashMap">
		select * from profiler where id=#{id} order by regdate desc
	</select>
	
	<select id="readOne" resultType="java.util.HashMap">
		select * from (select * from profiler where url=#{url} order by regdate desc) where rownum=1
	</select>
	
</mapper>
